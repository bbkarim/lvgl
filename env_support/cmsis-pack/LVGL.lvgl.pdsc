    <condition id="LVGL-Essential">
      <description>Require LVGL Essential Service</description>
      <require Cclass="Graphics" Cgroup="Essential"/>
    </condition>

    <condition id="Arm-2D">
      <description>Require Arm-2D Support</description>
      <require Cclass="Graphics" Cgroup="Arm-2D"/>
    </condition>

    <condition id="Arm-2D HAL">
        <description>Arm-2D HAL Dependencies</description>
        <require condition="LVGL-Essential"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="STM32-DMA2D"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="SWM341-DMA2D"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="NXP-PXP"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="NXP-VGLite"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="GD32-IPA"/>
    </condition>

    <condition id="STM32-DMA2D HAL">
        <description>STM32-DMA2D HAL Dependencies</description>
        <require condition="LVGL-Essential"/>
        <!-- <require Cclass="Device" Cgroup="DMA2D" Csub="HAL"/> -->
        <deny Cclass="Graphics" Cgroup="GPU" Csub="Arm-2D"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="SWM341-DMA2D"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="NXP-PXP"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="NXP-VGLite"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="GD32-IPA"/>
    </condition>

    <condition id="SWM341-DMA2D HAL">
        <description>SWM341-DMA2D HAL Dependencies</description>
        <require condition="LVGL-Essential"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="Arm-2D"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="STM32-DMA2D"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="NXP-PXP"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="NXP-VGLite"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="GD32-IPA"/>
    </condition>

    <condition id="NXP-PXP HAL">
        <description>NXP-PXP HAL Dependencies</description>
        <require condition="LVGL-Essential"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="Arm-2D"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="STM32-DMA2D"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="SWM341-DMA2D"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="NXP-VGLite"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="GD32-IPA"/>
    </condition>

    <condition id="NXP-VGLite HAL">
        <description>NXP-VGLite HAL Dependencies</description>
        <require condition="LVGL-Essential"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="Arm-2D"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="STM32-DMA2D"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="SWM341-DMA2D"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="NXP-PXP"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="GD32-IPA"/>
    </condition>

    <condition id="GD32-IPA HAL">
        <description>GD32-IPA HAL Dependencies</description>
        <require condition="LVGL-Essential"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="Arm-2D"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="STM32-DMA2D"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="SWM341-DMA2D"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="NXP-PXP"/>
        <deny Cclass="Graphics" Cgroup="GPU" Csub="NXP-VGLite"/>
    </condition>

    <condition id="LVGL-Extras">
      <description>Require Extras</description>
      <require Cclass="Graphics" Cgroup="Extras" Csub="Themes"/>
      <require Cclass="Graphics" Cgroup="Extras" Csub="Widgets"/>
      <require Cclass="Graphics" Cgroup="Extras" Csub="Layouts"/>
    </condition>
  </conditions>

